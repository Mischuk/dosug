      .services.l-bg
        .section-title.bold Услуги
        .row
          script(src="//api-maps.yandex.ru/2.1/?lang=ru_RU" type="text/javascript")
          script.
            //Map
              ymaps.ready(init);
              function init () {
                var myMap = new ymaps.Map("profile-map", {
                        center: [55.8083, 37.5868],
                        zoom: 16,
                        controls: ['zoomControl']
                    }),
                myPlacemark = new ymaps.Placemark([55.8083, 37.5868]);
                myMap.geoObjects.add(myPlacemark);
                myMap.behaviors.disable('scrollZoom');
              }
          #profile-map
