extends ../modules/layout/layout.jade

block title
  title Профиль

block breadcrumbs
  -
    var breadcrumbs = [
      {link: 'Все проститутки Москвы'},
      {link: 'Елизаветочка'}
    ]
  include ../modules/breadcrumbs/m_breadcrumbs

block content
  h1 Елизаветочка
  // Sidebar
  aside.m_sidebar.large
    .profile-sidebar
      .play-video
        a(href="#") Просмотреть видео
      .images
        a(href="girls/4.jpg").item: img(src="girls/4.jpg", alt="")
        a(href="girls/5.jpg").item: img(src="girls/5.jpg", alt="")
        a(href="girls/4.jpg").item: img(src="girls/4.jpg", alt="")
        a(href="girls/5.jpg").item: img(src="girls/5.jpg", alt="")
  // Partnership banners
  aside.m_partnership
    a(href="#"): img(src="http://placehold.it/221x300?text=Ads+1")
    a(href="#"): img(src="http://placehold.it/221x300?text=Ads+2")
    a(href="#"): img(src="http://placehold.it/221x300?text=Ads+3")
  // Content
  main
    .m_profile
      .lead.l-bg
        .m_girls
          .item
            .info
              .tel: a(href="#") +7 890 453 45 45
              .subway: a(href="#") Спортивная
              .verified
              .prices
                table
                  tr
                    th(colspan="2") Тариф:
                  tr
                    td 1 час
                    td <span class="sum">3 000</span> <span class="currency">руб</span>
                  tr
                    td 2 часа
                    td <span class="sum">5 000</span> <span class="currency">руб</span>
                  tr
                    td Ночь
                    td <span class="sum">10 000</span> <span class="currency">руб</span>
              .prices
                table
                  tr
                    th(colspan="2") Выезд:
                  tr
                    td 1 час
                    td -
                  tr
                    td 2 часа
                    td <span class="sum">6 000</span> <span class="currency">руб</span>
                  tr
                    td Ночь
                    td <span class="sum">12 000</span> <span class="currency">руб</span>
                  tr
                    td Анал
                    td <span class="sum">+2 000</span> <span class="currency">руб</span>
            .parameters
              .row
                .label Город:
                .value Москва
              .row
                .label Возраст:
                .value 25 лет
              .row
                .label Рост:
                .value 170 см
              .row
                .label Вес:
                .value 61 кг
              .row
                .label Размер одежды:
                .value 48
              .row
                .label Размер груди:
                .value 3
              .row
                .label Цвет волос:
                .value брюнетка
              .row
                .label Национальность:
                .value славянка
              .row
                .label Возраст партнера:
                .value 18-30 лет
              .row
                .label Интимная стрижка:
                .value полная депиляция
              .row
                .label Время работы:
                .value 00.00-24.00
              .row
                .label Отвечаю на:
                .value звонки и смс
              .row
                .label Дата регистрации:
                .value 2016-05-19
              .row
                .label Дата обновления:
                .value 2016-05-22
