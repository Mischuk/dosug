@font-face {
  font-family: 'dosug';
  src:  url('../fonts/dosug.eot');
  src:  url('../fonts/dosug.eot#iefix') format('embedded-opentype'),
    url('../fonts/dosug.ttf') format('truetype'),
    url('../fonts/dosug.woff') format('woff'),
    url('../fonts/dosug.svg#dosug') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'dosug' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-subway:before {
  content: "\e900";
}
.icon-search:before {
  content: "\e901";
}
